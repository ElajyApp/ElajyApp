{
  "expo": {
    "version": "1.0.0",
    "orientation": "portrait",
                    "icon": "./assets/images/icon-ar-back.png",
    "scheme": "elajypharmacyapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "image": "./assets/images/icon-ar-back.png",
        "prerendered": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-ar-back.png",
        "backgroundColor": "transparent",
        "monochromeImage": "./assets/images/icon-ar-back.png"
      },
      "icon": "./assets/images/icon-ar-back.png",
      "roundIcon": "./assets/images/icon-ar-back.png",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
